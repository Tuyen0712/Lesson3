string hackerrankInString(string s) {
    string target="hackerrank";
    int idx=0, count=0;
    for(int i=0; i<s.size(); i++){
        if(s[i]==target[idx]){
            count++;
            idx++;
        }
    }
    
    if(count==target.size()){
        return "YES";
    }else{
        return "NO";
    }
}
